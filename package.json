{
  "name": "pawnctl",
  "version": "1.0.0",
  "description": "CLI tool for open.mp/SA-MP development that helps manage packages and build PAWN projects. ",
  "keywords": [
    "pawn",
    "samp",
    "openmp",
    "package-manager",
    "build-tool"
  ],
  "homepage": "https://github.com/itsneufox/pawnctl#readme",
  "bugs": {
    "url": "https://github.com/itsneufox/pawnctl/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsneufox/pawnctl.git"
  },
  "license": "MIT",
  "author": "itsneufox",
  "type": "commonjs",
  "main": "dist/index.js",
  "bin": {
    "pawnctl": "bin/pawnctl.js"
  },
  "scripts": {
    "prebuild": "node scripts/prebuild.js",
    "copy-templates": "node scripts/copy-templates.js",
    "build": "tsc && npm run copy-templates",
    "start": "ts-node src/index.ts",
    "dev": "npm run build && npm link",
    "watch": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "npm run build",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
   
    "dev:stop": "npm unlink -g pawnctl || true",
    "dev:restart": "npm run dev:stop && npm run dev"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.4.0",
    "@octokit/rest": "^21.1.1",
    "@types/adm-zip": "^0.5.7",
    "@types/tar": "^6.1.13",
    "adm-zip": "^0.5.16",
    "chalk": "^5.6.0",
    "cli-progress": "^3.12.0",
    "commander": "^13.1.0",
    "ora": "^8.2.0",
    "simple-git": "^3.27.0",
    "tar": "^6.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/chalk": "^0.4.31",
    "@types/cli-progress": "^3.11.6",
    "@types/commander": "^2.12.0",
    "@types/node": "^22.13.10",
    "eslint": "^9.24.0",
    "prettier": "3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  }
}
